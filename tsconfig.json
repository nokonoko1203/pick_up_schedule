{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es5", "es6", "es7"],
        "outDir": "./dist",
        "rootDir": "./src",
        "module": "esnext",
        "downlevelIteration": true,
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node"
    },
    "include": ["./src/**/*"]
}
